package logdog.ErrorReport.DTO;

import java.util.ArrayList;

public class BaseErrorType {
	private ErrorUniqueID uid;

	private ArrayList<String> Callstack;
	
	public BaseErrorType(ErrorUniqueID uid, ArrayList<String> data) {
		super();
		this.uid = uid;
		Data = data;
	}

	public ErrorUniqueID getUid() {
		return uid;
	}

	public ArrayList<String> getCallstack() {
		return Data;
	}

}
