package com.logdog.server.TestCase;
import javax.ws.rs.Consumes;
import javax.ws.rs.POST;
import javax.ws.rs.Path;
import javax.ws.rs.core.MediaType;
import javax.ws.rs.core.Response;

import com.logdog.server.Model.GsonRefer.ClientReportData;

@Path("/TestCase")
public class GsonInputCase {

	@POST
	@Consumes(MediaType.APPLICATION_JSON)
	public Response addScore(ClientReportData clientData)
	{
		
		String result = "Error saved : " + clientData.ErrorName;
		return Response.status(201).entity(result).build();
 
	}

}
