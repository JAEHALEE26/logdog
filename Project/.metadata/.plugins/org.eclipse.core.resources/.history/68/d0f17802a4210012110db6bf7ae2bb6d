			try{
				List<MemberInfo> results = (List<MemberInfo>) q1.execute(id);
			}finally{
				q1.closeAll();
				pm.close();
			}
		*/
		return true;
	}
	
	public boolean InsertErrorType(String name, String classname)
	{
		jdoConnector = PMF.getPMF().getPersistenceManager();
		
			ErrorClassifiedInfo eType;
			try{
				eType = new ErrorClassifiedInfo(name,classname);
			
				jdoConnector.makePersistent(eType);
			}finally{
				q1.closeAll();
				pm.close();
			}
		return true;
	}
	
